<div>ログイン</div>

<div>
  <%= form_for :session, url: login_path do |f| %>
    <div>
      <%= f.label :email %>
      <%= f.text_field :email %>
    </div>
    
    <div>
      <%= f.label :password %>
      <%= f.text_field :password %>
    </div>
    
    <%= f.submit "ログイン" %>
    
  <% end %>
</div>